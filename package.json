{
  "name": "AgilePromoter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear": "rm -rf ./node_modules && npm cache clean --force && npm install && ./gradlew clean",
    "login": "npm login --scope=@OWNER --registry=https://npm.pkg.github.com",
    "setup": "npm install && npm run import-bundle",
    "import-bundle": "cp ./node_modules/@involvestecnologia/agile-promoter-react-bundle/android/index.android.bundle ./agilePromoter/src/main/assets/index.android.bundle ",
    "postinstall": "patch-package && rm -rf node_modules/react-native-screens/android/.cxx/"
  },
  "dependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.1.6",
    "@bam.tech/react-native-image-resizer": "3.0.10",
    "@instana/react-native-agent": "2.0.6",
    "@lodev09/react-native-exify": "0.2.7",
    "@react-native-community/cli-platform-android": "^11.3.0",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-native-firebase/app": "16.5.2",
    "@react-native-firebase/firestore": "16.5.2",
    "@react-navigation/native": "^6.1.6",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-background-fetch": "4.2.7",
    "react-native-blob-util": "0.19.4",
    "react-native-device-info": "10.0.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-file-viewer": "2.1.5",
